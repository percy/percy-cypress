{
  "name": "@percy/cypress",
  "description": "Cypress client library for visual testing with Percy",
  "version": "3.1.7",
  "license": "MIT",
  "author": "Perceptual Inc.",
  "repository": "https://github.com/percy/percy-cypress",
  "keywords": [
    "cypress",
    "percy",
    "visual testing"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "index.js",
    "createRegion.js",
