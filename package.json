{
  "name": "@percy/cypress",
  "version": "2.3.1",
  "description": "Cypress client library for visual regression testing with Percy (https://percy.io).",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "pretest": "yarn build",
    "test": "percy exec -- node run-tests.js",
    "test:debug": "LOG_LEVEL=debug yarn test",
    "clean": "rm -rf dist/",
    "build": "yarn clean && tsc",
    "prepublish": "yarn build",
    "lint": "tslint -p . -t stylish --fix",
    "cypress:open": "cypress open"
  },
  "resolutions": {
    "ecstatic": "3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percy/percy-cypress.git"
  },
  "keywords": [
    "cypress"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Perceptual Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/percy/percy-cypress/issues"
  },
  "homepage": "https://github.com/percy/percy-cypress#readme",
  "devDependencies": {
    "@oclif/tslint": "^3.1.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "cypress": "^4.1.0",
    "http-server": "^0.12.0",
    "semantic-release": "^17.0.4",
    "tslint": "^6.0.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@percy/agent": "~0",
    "axios": "^0.20.0"
  },
  "peerDependencies": {
    "cypress": "^3 || ^4"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
