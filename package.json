    "@percy/cli": "1.31.7",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "cypress": "^15.6.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "nyc": "^17.1.0",
    "tsd": "^0.33.0",
    "webpack": "^5.102.1"
